definition
	EVALUATION[at0000] matches {	-- *Pharmacist medication review(en)
		data matches {
			ITEM_TREE[at0001] matches {	-- *Tree(en)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0052] occurrences matches {0..1} matches {	-- *Medication list category(en)
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0053, 	-- *Admission(en)
									at0054, 	-- *Inpatient(en)
									at0055, 	-- *Outpatient(en)
									at0056, 	-- *Discharge(en)
									at0059]	-- *Short-term leave(en)
								}
							}
						}
					}
					CLUSTER[at0002] occurrences matches {0..*} matches {	-- *Medication item assessment(en)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0004] occurrences matches {0..*} matches {	-- *Related medication item(en)
								value matches {
									DV_EHR_URI matches {*}
								}
							}
							ELEMENT[at0003] occurrences matches {0..1} matches {	-- *No problem identified(en)
								value matches {
									DV_BOOLEAN matches {
										value matches {True}
									}
								}
							}
							ELEMENT[at0064] occurrences matches {0..1} matches {	-- *Overall recommendation(en)
								value matches {
									DV_TEXT matches {*}
								}
							}
							CLUSTER[at0007] occurrences matches {0..1} matches {	-- *Drug-related problem(en)
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0008] occurrences matches {0..*} matches {	-- *Category(en)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0012, 	-- *No indication for drug therapy(en)
													at0013, 	-- *No drug order for medical condition(en)
													at0014, 	-- *Inappropriate drug selection(en)
													at0015, 	-- *Inappropriate dose amount(en)
													at0016, 	-- *Inappropriate dose frequency(en)
													at0017, 	-- *Inappropriate route(en)
													at0018, 	-- *Inappropriate rate(en)
													at0019, 	-- *Prescribed drug not administered(en)
													at0020, 	-- *Experiencing ADRs or SEs(en)
													at0021, 	-- *Experiencing drug interactions(en)
													at0022, 	-- *Deviation from policy / protocol(en)
													at0065, 	-- *Administered via inappropriate route(en)
													at0066, 	-- *Allergies and hypersensitivities(en)
													at0067, 	-- *Food-drug interaction (actual or potential)(en)
													at0068, 	-- *Lab result-drug interaction(en)
													at0069]	-- *Disease-drug interaction(en)
												}
											}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- *Outcome(en)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0023, 	-- *Cost savings only(en)
													at0024, 	-- *Potential ADR / toxicity prevented(en)
													at0025]	-- *Enhanced therapeutic effect(en)
												}
											}
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- *Impact(en)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0026, 	-- *Potentially severe / high clinical significance(en)
													at0027, 	-- *Important / moderate clinical significance(en)
													at0028]	-- *Minor / low clinical significance(en)
												}
											}
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- *Recommendation(en)
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
							CLUSTER[at0029] occurrences matches {0..*} matches {	-- *Pharmacokinetic issue(en)
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0030] occurrences matches {0..*} matches {	-- *Category(en)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0060, 	-- *Loading dose(en)
													at0061, 	-- *Maintenance dose(en)
													at0062, 	-- *Inappropriate dosage(en)
													at0063]	-- *Experiencing ADRs or SEs(en)
												}
											}
										}
									}
									use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0002]/items[at0007]/items[at0009]	-- /data[*Tree(en)]/items[*Medication item assessment(en)]/items[*Drug-related problem(en)]/items[*Outcome(en)]
									use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0002]/items[at0007]/items[at0011]	-- /data[*Tree(en)]/items[*Medication item assessment(en)]/items[*Drug-related problem(en)]/items[*Impact(en)]
									use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0002]/items[at0007]/items[at0010]	-- /data[*Tree(en)]/items[*Medication item assessment(en)]/items[*Drug-related problem(en)]/items[*Recommendation(en)]
								}
							}
							CLUSTER[at0038] occurrences matches {0..*} matches {	-- *Patient related problem(en)
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0039] occurrences matches {0..*} matches {	-- *Category(en)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0040, 	-- *Adherence issue(en)
													at0041, 	-- *Patient counselling required(en)
													at0042, 	-- *Education material required(en)
													at0043]	-- *Experiencing ADRs or SEs(en)
												}
											}
										}
									}
									use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0002]/items[at0007]/items[at0009]	-- /data[*Tree(en)]/items[*Medication item assessment(en)]/items[*Drug-related problem(en)]/items[*Outcome(en)]
									use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0002]/items[at0007]/items[at0011]	-- /data[*Tree(en)]/items[*Medication item assessment(en)]/items[*Drug-related problem(en)]/items[*Impact(en)]
									use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0002]/items[at0007]/items[at0010]	-- /data[*Tree(en)]/items[*Medication item assessment(en)]/items[*Drug-related problem(en)]/items[*Recommendation(en)]
								}
							}
						}
					}
				}
			}
		}
	}


