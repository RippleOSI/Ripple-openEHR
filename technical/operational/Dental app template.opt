                        <term_definitions code="at0000">
                          <items id="description">Summary information about the prevalence of a risk factor, problem or diagnosis in all family members.</items>
                          <items id="text">Family prevalence</items>
                        </term_definitions>
                        <term_definitions code="at0030">
                          <items id="description">Narrative description about occurrence in family members.</items>
                          <items id="text">Description</items>
                        </term_definitions>
                        <term_definitions code="at0031">
                          <items id="comment">This cluster will be repeated for each relationship, family line, sex value or combination of all three. For example, the cluster will be repeated for any or all of first degree relative (without sex specified), first degree relative from maternal line; first degree male relative or first degree female relative; or all males, all females.</items>
                          <items id="description">Details about the numbers of family members affected.</items>
                          <items id="text">Affected family</items>
                        </term_definitions>
                        <term_definitions code="at0032">
                          <items id="description">The degree of relationship between the subject of care and a selected group of family members.</items>
                          <items id="text">Relationship</items>
                        </term_definitions>
                        <term_definitions code="at0033">
                          <items id="description">The biological sex of the family member/s.</items>
                          <items id="text">Biological sex</items>
                        </term_definitions>
                        <term_definitions code="at0034">
                          <items id="comment">This is the number of family members who are affected AND who also fit selected relationship, family line and sex criteria. Effectively this number is the numerator for calculation of an 'Affected ratio'.</items>
                          <items id="description">The number of family members known to be affected.</items>
                          <items id="text">Number affected</items>
                        </term_definitions>
                        <term_definitions code="at0037">
                          <items id="description">50% genetic share with the subject - for example, parent, sibling or child.</items>
                          <items id="text">First degree relative</items>
                        </term_definitions>
                        <term_definitions code="at0038">
                          <items id="description">25% genetic share with the subject -  for example, grandparent, aunt, uncle, niece, nephew, grandchildren. and half siblings.</items>
                          <items id="text">Second degree relative</items>
                        </term_definitions>
                        <term_definitions code="at0039">
                          <items id="description">12.5% genetic share with the subject - for example, great grandparent, great aunt, great uncle, first cousin, children of nieces and nephews, and great grandchildren.</items>
                          <items id="text">Third degree relative</items>
                        </term_definitions>
                        <term_definitions code="at0040">
                          <items id="description">Related through the subject's mother.</items>
                          <items id="text">Maternal line</items>
                        </term_definitions>
                        <term_definitions code="at0041">
                          <items id="description">Related through the subject's father.</items>
                          <items id="text">Paternal line</items>
                        </term_definitions>
                        <term_definitions code="at0042">
                          <items id="description">All genetically-related family members.</items>
                          <items id="text">Genetic family</items>
                        </term_definitions>
                        <term_definitions code="at0043">
                          <items id="description">Family member who is biologically male.</items>
                          <items id="text">Male</items>
                        </term_definitions>
                        <term_definitions code="at0044">
                          <items id="description">Family member who is biologically female.</items>
                          <items id="text">Female</items>
                        </term_definitions>
                        <term_definitions code="at0050">
                          <items id="description">All non-genetic family members.</items>
                          <items id="text">Non-genetic family</items>
                        </term_definitions>
                        <term_definitions code="at0051">
                          <items id="comment">Only for use with genetic family members.</items>
                          <items id="description">Identification of the maternal or paternal family line in the relationship.</items>
                          <items id="text">Family line</items>
                        </term_definitions>
                        <term_definitions code="at0052">
                          <items id="description">Family member who either has the biological attributes of both sexes or lacks some of the biological attributes considered necessary to be defined as one or the other sex.</items>
                          <items id="text">Indeterminate/Intersex/Unspecified</items>
                        </term_definitions>
                        <term_definitions code="at0054">
                          <items id="comment">This is the number of family members who potentially could be affected AND who also fit selected relationship, family line and sex criteria. Effectively this number is the denominator for calculation of an 'Affected ratio'.</items>
